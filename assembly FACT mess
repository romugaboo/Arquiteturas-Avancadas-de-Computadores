addi sp, x0, 1000
addi x10, x0, 4  # Fatorial do valor 4 (ou qualquer outro)
addi x9, x0, 1 
jal x1, FACT
jal x0, END

FACT:
    bne x10, x9, ELSE
    sub x10, x10, x10     #Fatorial de 1 é 1
    addi x10, x10, 1
    beq x0, x0, EXIT
ELSE: 
EXIT:

bne x10, x0, ELSE2
    sub x10, x10, x10     #Fatorial de 0 é 1
    addi x10, x10, 1
    beq x0, x0, EXIT2
ELSE2: 
EXIT2:



END:
